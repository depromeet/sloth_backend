{
    "containerDefinitions": [
        {
            "name": "sloth-dev",
            "image": "678547374742.dkr.ecr.ap-northeast-2.amazonaws.com/sloth-ecr:dev",
            "cpu": 0,
            "portMappings": [
                {
                    "name": "sloth-dev-8081-tcp",
                    "containerPort": 8081,
                    "protocol": "tcp"
                }
            ],
            "essential": true,
            "environment": [
                {
                    "name": "nanagong.enc.password",
                    "value": "nanagong-backend!@#"
                },
                {
                    "name": "SPRING_PROFILES_ACTIVE",
                    "value": "real1"
                },
                {
                    "name": "ECS_ENABLE_EXECUTE_COMMAND",
                    "value": "true"
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-create-group": "true",
                    "awslogs-group": "/ecs/sloth-dev-task-definition",
                    "awslogs-region": "ap-northeast-2",
                    "awslogs-stream-prefix": "ecs"
                }
            }
        }
    ],
    "family": "sloth-dev-task-definition",
    "taskRoleArn": "arn:aws:iam::678547374742:role/sloth-ecs-task-role",
    "executionRoleArn": "arn:aws:iam::678547374742:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "placementConstraints": [],
    "requiresCompatibilities": [
        "EC2"
    ],
    "cpu": "1024",
    "memory": "3072",
    "tags": [
        {
            "key": "env",
            "value": "dev"
        },
        {
            "key": "sloth",
            "value": "true"
        }
    ]
}